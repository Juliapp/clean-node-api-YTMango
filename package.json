{
  "name": "clean-node-api-YTMongo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juliapp/clean-node-api-YTMongo.git"
  },
  "author": "Juli <aragaopintojuli@gmail.com>",
  "license": "MIT",
  "description": "Curso gratuito de API em Node com Clean Architecture e TDD",
  "scripts": {
    "test": "jest --colors --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage "
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "validator": "^13.5.2"
  }
}
